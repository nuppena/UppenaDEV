{
  "_version": "1.65.0",
  "sap.app": {
    "id": "pcf.com.acc.packaging",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.4"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "e2e8baa5-da36-4a01-b4d6-662501e657ca"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/packaging-efcalculation/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      
      "locDataSource": {
				"uri": "model/localModel.json",
				"type": "JSON"
		}
    },
    "crossNavigation": {
      "inbounds": {
        "zbupackaging-create": {
          "semanticObject": "zbupackaging",
          "action": "display",
          "title": "{{zbupackaging-create.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "pcf.com.acc.packaging.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "useBatch": false
        }
      },
      "locModel": {
      "type": "sap.ui.model.json.JSONModel",
      "dataSource": "locDataSource"
    }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "pcf.com.acc.packaging.view",
        "async": true,
        "viewPath": "pcf.com.acc.packaging.view"
      },
      "routes": [
        {
          "name": "RoutePackaging",
          "pattern": ":?query:",
          "target": [
            "TargetPackaging"
          ]
        }
      ],
      "targets": {
        "TargetPackaging": {
          "id": "Packaging",
          "name": "Packaging"
        }
      }
    },
    "rootView": {
      "viewName": "pcf.com.acc.packaging.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "zbuPackagingrouter"
  }
}
